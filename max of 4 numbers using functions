#include <stdio.h>

int  max(int a, int b, int c, int d)
{
    if(a>b && a>c && a>d)
    {
       // printf("%d",a);
       return a;
    }
    else if(b>a && b>c && b>d)
    {
        //printf("%d",b);
        return b;
    }
    else if(c>a && c>b && c>d)
    {
       // printf("%d",c);
       return c;
    }
    else {
    {
       // printf("%d",d);
       return d;
    }
    }
}

int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans =max(a, b, c, d);
    printf("%d", ans);
    
    return 0;
}
